---
import Redirect from "../../layouts/Redirect.astro";
import { parseCookies } from "../../cookies.js";

const cookies = parseCookies(Astro.request.headers);
var href:string;
if (cookies.has("category") && cookies.has("class")) {
    href = "/open/" + cookies.get("category");
} else if (hasCookie("class")) {
    href = "/select/category#" + cookies.get("class");
} else {
    href = "/select/year";
}

return Astro.redirect(href)
---

<Redirect href={href}></Redirect>