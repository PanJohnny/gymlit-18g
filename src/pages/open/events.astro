---
import {canUseCookies, sendCookie, getCookie} from "../../cookies.js"
import Redirect from "../../layouts/Redirect.astro";

const c = getCookie("class", Astro.request.headers)
if (!c) {
	return Astro.redirect("/select/year")
}

if (canUseCookies(Astro.request.headers))
	sendCookie("category", "events", 1, Astro.response.headers)

const href = "/events/"+c
return Astro.redirect(href)
---

<Redirect href={href}>

</Redirect>