<html>
    <head>
        <script src="/cookieUtil.js" is:inline></script>

    <script is:inline>
        if (window.location.hash.length > 2) {
            const hash = window.location.hash.substring(1)

            if (hash == "cookies") {
                deleteAllCookies()
                window.localStorage.clear()
                window.location.href = "/"
            } else if(hash == "logout") {
                eraseCookie("auth")
                window.location.href = "/settings"
            }
        }
    </script>
    </head>
</html>